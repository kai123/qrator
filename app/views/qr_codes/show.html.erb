<div class"container">
	<div class="row">
		<div class="col-md-9 col-md-offset-3">
			<p id="notice">
				<h1><%= notice %></h1>
			</p>

			<p>
				<strong>Qrcode SVG:</strong>
				<br>
				<br>
				<%= image_tag qr_code_path(@qr_code, format: :svg), class: "qr_code_graph" %>
				<br>
				<%#= image_tag qr_code_path(@qr_code, format: :png), class: "qr_code_graph" %>
				<br>
				<%#= image_tag qr_code_path(@qr_code, format: :jpeg), class: "qr_code_graph" %>
				<br>
			</p>

			<p>
				<strong>Qrlink: </strong>
				<%= @qr_code.qrlink %>
			</p>

			<p>
				<strong>Basic Url: </strong>
				<%= @qr_code.url %>
			</p>

			<p>
				<strong>Src: </strong>
				<%= @qr_code.src %>
			</p>

			<p>
				<strong>Medium: </strong>
				<%= @qr_code.medium %>
			</p>

			<p>
				<strong>Campaign: </strong>
				<%= @qr_code.campaign %>
			</p>

			<p>
				<strong>Content: </strong>
				<%= @qr_code.content %>
			</p>

			<p>
				<strong>Term: </strong>
				<%= @qr_code.term %>
			</p>

			<p>
				<strong>Full Url: </strong>
				<%= @qr_code.full_url %>
			</p>

		</div>
	</div>
</div>


<div class="row">
	<div class="col-md-9 col-md-offset-3">
		<button type="button" class="btn btn-default"><%= link_to "Listenansicht", qr_codes_path %></button>
	<div>
	</br>
</div>
