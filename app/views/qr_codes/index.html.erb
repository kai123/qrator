<h1>Listing qr_codes</h1>

<%= @qrcode.inspect %>

<table>
  <thead>
    <tr>
      <th>Qrcode</th>
      <th>Qrlink</th>
      <th>Url</th>
      <th>Src</th>
      <th>Medium</th>
      <th>Campaign</th>
      <th>Content</th>
      <th>Term</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qr_codes.each do |qr_code| %>
      <tr>
        <td><%= qr_code.qrcode %></td>
        <td><%= qr_code.qrlink %></td>
        <td><%= qr_code.url %></td>
        <td><%= qr_code.src %></td>
        <td><%= qr_code.medium %></td>
        <td><%= qr_code.campaign %></td>
        <td><%= qr_code.content %></td>
        <td><%= qr_code.term %></td>
        <td><%= link_to 'Show', qr_code %></td>
        <td><%= link_to 'Edit', edit_qr_code_path(qr_code) %></td>
        <td><%= link_to 'Destroy', qr_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Neuer QR-Code', new_qr_code_path %>
